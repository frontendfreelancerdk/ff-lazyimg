{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "ff",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "ff",
      "kebab-case"
    ],
    "max-line-length": [
      false,
      140
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-whitespace"
    ]
  }
}
